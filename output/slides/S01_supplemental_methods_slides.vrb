\begin{block}{S1.9.3 Network Construction Queries}
\protect\phantomsection\label{s1.9.3-network-construction-queries}
\textbf{Room-based edges:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way1\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way2\_id, w1.mene }\KeywordTok{as}\NormalTok{ room}
\KeywordTok{FROM}\NormalTok{ ways w1}
\KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.mene }\OperatorTok{=}\NormalTok{ w2.mene}
\KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{\textless{}}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.mene }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}\NormalTok{;}
\end{Highlighting}
\end{Shaded}

\textbf{Partner-based edges:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way1\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way2\_id, w1.dialoguewith }\KeywordTok{as}\NormalTok{ partner}
\KeywordTok{FROM}\NormalTok{ ways w1}
\KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.dialoguewith }\OperatorTok{=}\NormalTok{ w2.dialoguewith}
\KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{\textless{}}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.dialoguewith }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}\NormalTok{;}
\end{Highlighting}
\end{Shaded}

\textbf{Type-based edges:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way1\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way2\_id, w1.dialoguetype }\KeywordTok{as} \KeywordTok{type}
\KeywordTok{FROM}\NormalTok{ ways w1}
\KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.dialoguetype }\OperatorTok{=}\NormalTok{ w2.dialoguetype}
\KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{\textless{}}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.dialoguetype }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}\NormalTok{;}
\end{Highlighting}
\end{Shaded}
\end{block}

\begin{block}{S1.9.4 Centrality Analysis Queries}
\protect\phantomsection\label{s1.9.4-centrality-analysis-queries}
\textbf{Degree centrality calculation:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT}\NormalTok{ way\_id, }\FunctionTok{COUNT}\NormalTok{(}\OperatorTok{*}\NormalTok{) }\KeywordTok{as} \KeywordTok{degree}
\KeywordTok{FROM}\NormalTok{ (}
    \KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ connected\_way}
    \KeywordTok{FROM}\NormalTok{ ways w1}
    \KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.mene }\OperatorTok{=}\NormalTok{ w2.mene}
    \KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{!=}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.mene }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}
    \KeywordTok{UNION}
    \KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ connected\_way}
    \KeywordTok{FROM}\NormalTok{ ways w1}
    \KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.dialoguewith }\OperatorTok{=}\NormalTok{ w2.dialoguewith}
    \KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{!=}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.dialoguewith }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}
    \KeywordTok{UNION}
    \KeywordTok{SELECT}\NormalTok{ w1.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ way\_id, w2.}\KeywordTok{ID} \KeywordTok{as}\NormalTok{ connected\_way}
    \KeywordTok{FROM}\NormalTok{ ways w1}
    \KeywordTok{JOIN}\NormalTok{ ways w2 }\KeywordTok{ON}\NormalTok{ w1.dialoguetype }\OperatorTok{=}\NormalTok{ w2.dialoguetype}
    \KeywordTok{WHERE}\NormalTok{ w1.}\KeywordTok{ID} \OperatorTok{!=}\NormalTok{ w2.}\KeywordTok{ID} \KeywordTok{AND}\NormalTok{ w1.dialoguetype }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}
\NormalTok{)}
\KeywordTok{GROUP} \KeywordTok{BY}\NormalTok{ way\_id}
\KeywordTok{ORDER} \KeywordTok{BY} \KeywordTok{degree} \KeywordTok{DESC}\NormalTok{;}
\end{Highlighting}
\end{Shaded}
\end{block}

\begin{block}{S1.9.5 Text Analysis Queries}
\protect\phantomsection\label{s1.9.5-text-analysis-queries}
\textbf{Ways with examples:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT} \KeywordTok{ID}\NormalTok{, way, }\FunctionTok{LENGTH}\NormalTok{(examples) }\KeywordTok{as}\NormalTok{ example\_length, examples}
\KeywordTok{FROM}\NormalTok{ ways}
\KeywordTok{WHERE}\NormalTok{ examples }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}} \KeywordTok{AND} \FunctionTok{LENGTH}\NormalTok{(examples) }\OperatorTok{\textgreater{}} \DecValTok{0}
\KeywordTok{ORDER} \KeywordTok{BY}\NormalTok{ example\_length }\KeywordTok{DESC}\NormalTok{;}
\end{Highlighting}
\end{Shaded}

\textbf{Average example length by dialogue type:}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{SELECT}\NormalTok{ dialoguetype, }
       \FunctionTok{AVG}\NormalTok{(}\FunctionTok{LENGTH}\NormalTok{(examples)) }\KeywordTok{as}\NormalTok{ avg\_length,}
       \FunctionTok{COUNT}\NormalTok{(}\OperatorTok{*}\NormalTok{) }\KeywordTok{as} \FunctionTok{count}
\KeywordTok{FROM}\NormalTok{ ways}
\KeywordTok{WHERE}\NormalTok{ examples }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}} \KeywordTok{AND}\NormalTok{ dialoguetype }\OperatorTok{!=} \StringTok{\textquotesingle{}\textquotesingle{}}
\KeywordTok{GROUP} \KeywordTok{BY}\NormalTok{ dialoguetype}
\KeywordTok{ORDER} \KeywordTok{BY}\NormalTok{ avg\_length }\KeywordTok{DESC}\NormalTok{;}
\end{Highlighting}
\end{Shaded}
\end{block}
\end{block}

\begin{block}{S1.10 Limitations and Assumptions}
\protect\phantomsection\label{s1.10-limitations-and-assumptions}
\begin{block}{Data Limitations}
\protect\phantomsection\label{data-limitations}
\begin{itemize}
\tightlist
\item
  Not all ways have complete metadata
\item
  Some room assignments may be missing or provisional
\item
  Dialogue partner information varies in completeness
\item
  Examples and descriptions vary in detail
\end{itemize}
\end{block}

\begin{block}{Analysis Assumptions}
\protect\phantomsection\label{analysis-assumptions}
\begin{itemize}
\tightlist
\item
  Ways are treated as discrete entities (though they may overlap)
\item
  Relationships are binary (present/absent) rather than weighted
\item
  Network structure captures all important relationships
\item
  Statistical patterns reflect meaningful structure
\end{itemize}
\end{block}

\begin{block}{Methodological Limitations}
\protect\phantomsection\label{methodological-limitations}
\begin{itemize}
\tightlist
\item
  Quantitative analysis may miss qualitative nuances
\item
  Network analysis based on explicit database relationships
\item
  Text analysis limited by available descriptions
\item
  Visualization choices may emphasize certain aspects
\end{itemize}

These limitations are acknowledged and addressed where possible through
multiple analysis methods and careful interpretation.
\end{block}
\end{block}
